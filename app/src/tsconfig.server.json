{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app-server",
    "baseUrl": ".",
    "module": "commonjs"
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule",
    "paths": {
      "schemes": [
        "../dist/schemes"
      ],
      "shared": [
        "../dist/shared"
      ],
      "core": [
        "../dist/core"
      ]
    }
  }
}