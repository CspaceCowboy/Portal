{
  "name": "midnight-lizard-web-portal",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/pavel-agarkov/Midnight-Lizard-Web-Portal.git",
  "author": "Pavel Agarkov",
  "description": "Midnight Lizard Web Portal",
  "scripts": {
    "dev-test": "SET ENV=test&& karma start ClientApp/test/karma.conf.js",
    "ci-test": "SET ENV=test&& karma start --single-run --browsers ChromeCI --reporters progress,tfs ClientApp/test/karma.conf.js",
    "docker-test": "cd ..&&docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "docker-build": "cd ..&&docker-compose -f docker-compose.build.yml up --build --abort-on-container-exit",
    "prod-build": "SET ENV=prod&& webpack -p --env.prod",
    "dev-build": "SET ENV=dev&& webpack",
    "vendor-prod-build": "npm run --prefix ./node_modules/midnight-lizard-vendor prod-build",
    "vendor-dev-build": "npm run --prefix ./node_modules/midnight-lizard-vendor dev-build"
  },
  "dependencies": {
    "midnight-lizard-vendor": "2.5.0"
  },
  "devDependencies": {
    "karma": "1.7.1",
    "midnight-lizard-testing": "1.2.0"
  }
}
